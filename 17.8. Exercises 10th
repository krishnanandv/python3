
big_list = [[['one', 'two'], ['seven', 'eight']], [['nine', 'four'], ['three', 'one']], [['two', 'eight'], ['seven', 'four']], [['five', 'one'], ['four', 'two']], [['six', 'eight'], ['two', 'seven']], [['three', 'five'], ['one', 'six']], [['nine', 'eight'], ['five', 'four']], [['six', 'three'], ['four', 'seven']]]
word_counts={}

for l1 in big_list:
    for l2 in l1:
        for word in l2:
            if word in word_counts:
                word_counts[word] += 1          
            else:
                word_counts[word] = 1
print(word_counts)                    
            
