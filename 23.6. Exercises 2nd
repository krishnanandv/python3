
def lengths(strings):
    
    """lengths takes a list of strings as input and returns a list of numbers that are the lengths
    of strings in the input list. Use manual accumulation!"""
    # fill in this function's definition to make the test pass.
    lenn=[]
    for str in strings:
        #print(str)
        val= len(str)
        #print(val)
        lenn.append(val)
        print(lenn)
    return(lenn)
